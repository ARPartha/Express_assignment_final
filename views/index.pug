  // modals
  #priceModal.modal.fade(tabindex='-1' aria-labelledby='exampleModalLabel' aria-hidden='true')
    .modal-dialog
      .modal-content
        .modal-header
          h5#exampleModalLabel.modal-title
            span Price Range: 
          button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
        .modal-body
          .price-range-slider
            p.range-value
              input#amount(type='text' readonly='')
            #slider-range.range-bar
        .modal-footer
          button.btn.btn-primary(type='button' data-bs-dismiss='modal' style='background-color: #f7941d' onclick='rangeprice()')
            | Apply
  // price modal
  // Guest modal
  #guestModal.modal.fade(tabindex='-1' aria-labelledby='exampleModalLabel' aria-hidden='true')
    .modal-dialog
      .modal-content
        .modal-header
          h5#exampleModalLabel.modal-title
            span Guest: 
          button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
        .modal-body
          .parent.container.d-flex.justify-content-center.align-items-center.h-100
            .child
              button.redondo(style='font-size: large' onclick='decrement()')
                | -
              span#updateGuestInfo(style='margin: 2em; font-size: large')
                | 0
              button.redondo(onclick='increment()') +
        .modal-footer
          button.btn.btn-primary(type='button' data-bs-dismiss='modal' style='background-color: #f7941d' onclick='guestupdate()')
            | Apply
